{
    "include": [
        "./src/**/*.ts"
    ],
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "esnext",
        "target": "es5",
        "moduleResolution": "node",
        "allowJs": true,
        "types": ["vuetify"],
        "lib": [
            "dom",
            "es5",
            "es2015.promise"
        ]
    }
}