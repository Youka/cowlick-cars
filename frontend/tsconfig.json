{
    "include": [
        "./src/**/*.ts"
    ],
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "moduleResolution": "node",
        "allowJs": true
    }
}